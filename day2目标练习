<script>
for (var i = 10; i >= 1; i--) { 
            for (var k = 0; k < 10 - i; k++) {
                console.log('&nbsp;&nbsp;&nbsp;');
            }
            for (var j = 0; j < i; j++) {   
                console.log('$&nbsp;&nbsp;&nbsp;&nbsp;');
            }
            console.log('<br />');
        }
</script>
</body>
